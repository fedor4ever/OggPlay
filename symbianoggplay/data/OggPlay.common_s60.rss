
#include "oggdialogs.rh"

RESOURCE EIK_APP_INFO
{
  menubar=r_oggplay_menubar;
  hotkeys=r_oggplay_hotkeys;
  cba=R_AVKON_SOFTKEYS_OPTIONS_BACK;
}

RESOURCE HOTKEYS r_oggplay_hotkeys
    {
    control=
        {
        HOTKEY { command=EAknCmdExit; key='e'; }
        };
    }
       
RESOURCE MENU_BAR r_oggplay_menubar
{
  titles= { 
    MENU_TITLE { menu_pane=r_file_menu; txt="OggPlay!"; }
  };
}
  
RESOURCE TBUF r_ogg_version { buf=ogg_txt_version; }

RESOURCE DIALOG r_dialog_about {
  title   = ogg_about_oggplay;
  buttons =  R_AVKON_SOFTKEYS_BACK; 
  flags   = EEikDialogFlagWait;
  items = {
    DLG_LINE {
	type = EEikCtLabel;
	id = EOggLabelAboutVersion;
	control = LABEL 
	{
		txt=ogg_version_txt;
		horiz_align=EEikLabelAlignHCenter;
	};
    } ,
    DLG_LINE {
	type = EEikCtLabel;
	id = EOggLabelAboutVersion2;
	control = LABEL 
	{
		txt=ogg_c_author;
		horiz_align=EEikLabelAlignHLeft;
	};
    } ,
    DLG_LINE {
	type = EEikCtLabel;
	id = EOggLabelAboutVersion3;
	control = LABEL 
	{
		txt=ogg_link_txt;
		horiz_align=EEikLabelAlignHLeft;
	};
    } ,
    DLG_LINE {
	type = EEikCtLabel;
	id = EOggLabelAboutVersion4;
	control = LABEL 
	{
		txt=ogg_vorbis_c;
		horiz_align=EEikLabelAlignHLeft;
	};
    } ,
    DLG_LINE {
	type = EEikCtLabel;
	id = EOggLabelAboutVersion5;
	control = LABEL 
	{
		txt=ogg_translation;
		horiz_align=EEikLabelAlignHLeft;
	};
    } ,
    DLG_LINE {
	type = EEikCtLabel;
	id = EOggLabelAboutVersion6;
	control = LABEL 
	{
		txt=ogg_port;
		horiz_align=EEikLabelAlignHLeft;
	};
    }	
  };
}


RESOURCE DIALOG r_dialog_hotkey {
  title   = "Optionen";
  buttons = R_EIK_BUTTONS_CANCEL_DONE;
  flags   = EEikDialogFlagWait;
  items = {
    DLG_LINE {
      type = EEikCtChoiceList;
      prompt = "Schnellstart:";
      id = EOggOptionsHotkey;
      control = CHOICELIST { };
    } ,
    DLG_LINE {
      type = EQikCtTimeEditor;
      prompt = "Alarm:";
      id = EOggOptionsAlarmTime;
      control = QIK_TIME_EDITOR {
        minTime = TIME { };
        maxTime = TIME {
          second = 59;
          minute = 59;
          hour = 23; };
        flags = EQikTimeForce24HourFormat; 
      };
    } ,
    DLG_LINE {
      type = EEikCtCheckBox;
      prompt = "Aktiv:";
      id = EOggOptionsAlarmActive;
    }
  };
}

